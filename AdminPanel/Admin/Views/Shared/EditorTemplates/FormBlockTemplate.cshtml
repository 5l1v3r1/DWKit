@using Admin.Helpers
@using OptimaJet.Meta.Objects
@model object

@Html.DevExpress().ComboBox(
    settings =>
    {
        CurrentSettings.LoadComboBoxSettings(settings, ViewData, Url.Action("Edit", "FormBlockTemplate"));
        settings.CallbackRouteValues = new { Controller = "FormBlockTemplate", Action = "EditorCombox", PropertyName = ViewData.ModelMetadata.PropertyName, IsNullableValueType = ViewData.ModelMetadata.IsNullableValueType };
        settings.Properties.Columns.Add("Name", Resources.Resource.Name);
        settings.Properties.Columns.Add("Comment", Resources.Resource.Comment);
    }
               ).BindList(MetaFormBlockTemplateHelper.GetAll()).Bind(Model).GetHtml()